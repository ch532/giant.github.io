{
  "name": "Connect Gold",
  "short_name": "ConnectGold",
  "start_url": "/",
  "display": "fullscreen",
  "background_color": "#ffffff",
  "theme_color": "#000000",
  "orientation": "portrait",
  "icons": [
    {
      "src": "/icons/icon-192.png",
      "sizes": "192x192",
      "type": "connectgold-1.png"
    },
    {
      "src": "/icons/icon-512.png",
      "sizes": "512x512",
      "type": "gold2-png"
    }
  ],
  "categories": [
    "business",
    "news",
    "productivity",
    "travel",
    "utilities",
    "weather"
  ],
  "share_target": {
    "method": "POST",
    "action": "\"action\": \"/share\"",
    "enctype": "\"enctype\": \"multipart/form-data\"",
    "params": {
      "title": "\"title\": \"title\"",
      "text": "\"text\": \"text\"",
      "url": "\"url\": \"url\""
      "file_handlers": [
  {
    "action": "/open",
    "accept": {
      "text/plain": [".txt"],
      "application/json": [".json"],
      "image/*": [".png", ".jpg", ".jpeg"]
    }
  }
}
